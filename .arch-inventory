source ^(\.gdbinit)$

# Ignore generated binaries
#
backup ^(snogray|snogcvt|snogdiff|snogsamp)$

# Ignore mesh files used for test scenes that are too large to put in
# the repository.
#
backup ^((teapot)\.msh)$

# Ignore files generated by configure
#
backup ^(Makefile)$
backup ^(config\.status|config\.cache|autom4te\.cache|config\.h|config\.log)$
backup ^(stamp-h.*)$

# Ignore files generated by autoconf, automake etc.
#
backup ^(Makefile\.in|Makefile|aclocal\.m4|configure|config\.h\.in)$

# Ignore boilerplate and system-install stuff
#
backup ^(depcomp|install-sh|missing|mkinstalldirs|config\.(guess|sub))$

junk ^(\.deps)$

